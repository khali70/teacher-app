{
  "expo": {
    "name": "Coures_App",
    "slug": "coures_app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#abb8c3"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#abb8c3"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "this is a prototype app for courses app using youtube as video Data Base"
  }
}
